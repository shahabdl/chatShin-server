datasource db {
    provider = "mongodb"
    url      = env("MONGODB_URI")
}

generator client {
    provider = "prisma-client-js"
}

model User {
    id            String    @id @default(auto()) @map("_id") @db.ObjectId
    name          String?
    username      String?   @unique
    password      String?
    email         String?   @unique
    emailVerified DateTime?
    image         String?
    initialized   Boolean   @default(false)
}
